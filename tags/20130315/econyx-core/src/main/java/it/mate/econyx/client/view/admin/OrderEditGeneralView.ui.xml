<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui"
       xmlns:e="urn:import:it.mate.econyx.client.ui.editors" xmlns:gwtcommons="urn:import:it.mate.gwtcommons.client.ui" >

  <g:HTMLPanel>
    <e:OrderEditor ui:field="editor"/>
    <gwtcommons:Spacer height="0.8em"/>
    <g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE">
      <g:Label text="Produttore:" width="12em" horizontalAlignment="ALIGN_RIGHT"/> <g:Label ui:field="produttoreLbl"/>
    </g:HorizontalPanel>
    <gwtcommons:Spacer height="1em"/>
    <g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE">
      <g:Label text="Importo listino:" width="12em" horizontalAlignment="ALIGN_RIGHT"/> <g:Label ui:field="importoCalcolatoLbl"/>
    </g:HorizontalPanel>
    <gwtcommons:Spacer height="1em"/>
    <g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" ui:field="importoTotalePanel">
      <g:Label text="Importo alla consegna:" width="12em" horizontalAlignment="ALIGN_RIGHT"/> <g:Label ui:field="importoTotaleLbl"/>
    </g:HorizontalPanel>
    <gwtcommons:Spacer height="1em"/>
    <g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" ui:field="deliveryInformationsPanel">
      <g:Label text="Informazioni per il ritiro:" width="12em" horizontalAlignment="ALIGN_RIGHT"/> <g:Label ui:field="deliveryInformationsLbl"/>
    </g:HorizontalPanel>
    <gwtcommons:Spacer height="2em"/>
    <g:Button ui:field="closeBtn" text="Chiudi ordine!"/>
    <g:Anchor ui:field="printAnchor" text="Stampa ordine"/>
  </g:HTMLPanel>

</ui:UiBinder>