<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="ui-wrapper-panel">
     
    <o:OnsPage>
    
      <o:OnsToolbar>
        <div class="center">First Toolbar</div>
      </o:OnsToolbar>
  
      <g:VerticalPanel width="100%" horizontalAlignment="center">
        <o:OnsButton text="Settings" ui:field="btnSettings"></o:OnsButton>
        <g:Label ui:field="homeLbl" addStyleNames="ui-home-lbl"/>
      </g:VerticalPanel>
      
    </o:OnsPage>
     
    
  </g:HTMLPanel>
  
</ui:UiBinder>