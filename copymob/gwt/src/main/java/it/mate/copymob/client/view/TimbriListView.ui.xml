<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="app-page-wrapper-panel">
  
    <o:OnsPage>
    
      <o:OnsToolbar>
        <div class="left">
          <!-- 
          <o:OnsBackButton text="Back"/>
           -->
        </div>
        <div class="center">Elenco Timbri</div>
      </o:OnsToolbar>
      
      <o:OnsHorizontalPanel width="100%" height="100%" horizontalAlignment="center" addStyleNames="app-timbri-carousel-panel">
        <o:OnsCarousel addStyleNames="app-timbri-carousel" autoscroll="true" ui:field="carousel" swipeable="true"/>
      </o:OnsHorizontalPanel>

      <o:OnsButton text="Acquista" ui:field="btnBuy" addStyleNames="app-timbri-choose-btn"/>
      
    </o:OnsPage>
    
  </g:HTMLPanel>
  
</ui:UiBinder>