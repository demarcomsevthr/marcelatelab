<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       xmlns:m="urn:import:com.googlecode.mgwt.ui.client.widget"
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:p="urn:import:it.mate.phgcommons.client.ui" 
       xmlns:ph="urn:import:it.mate.phgcommons.client.ui.ph">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="ui-homePanel">
  
    <g:Label text="Home view"></g:Label>
    
    <p:TouchButton text="Sign In" ui:field="signInBtn"/>
    
    <p:TouchButton text="Test" ui:field="testBtn"/>
    
    <p:TouchButton text="Touch" ui:field="touchBtn"/>
    
    <ph:PhDateBox width="10em" ui:field="dateBox"/>
    
    <ph:PhTimeBox width="10em" ui:field="timeBox"/>

    <ph:PhCalendarBox width="10em" ui:field="calBox"/>
    
    <p:TouchButton text="Calendar Test" ui:field="calTest"/>
    
    <g:VerticalPanel ui:field="outputPanel">
      <g:Label ui:field="outputLbl"></g:Label>
    </g:VerticalPanel>

  </g:HTMLPanel>
  
</ui:UiBinder>