<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       xmlns:m="urn:import:com.googlecode.mgwt.ui.client.widget"
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:p="urn:import:it.mate.phgcommons.client.ui" xmlns:ph="urn:import:it.mate.phgcommons.client.ui.ph">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="homePanel">
  
    <g:Label text="Home view"></g:Label>
    
    <p:SmartButton text="Sign In"></p:SmartButton>
    
    <p:SmartButton text="Test" ui:field="testBtn"></p:SmartButton>
    
    <g:VerticalPanel ui:field="outputPanel">
      <g:Label ui:field="outputLbl"></g:Label>
    </g:VerticalPanel>

    <!--      
    <s:JQDateBox></s:JQDateBox>
    <s:JQFlipBox></s:JQFlipBox>
     -->
     
    <ph:PhDateBox width="10em" ui:field="dateBox"></ph:PhDateBox>
    
    <ph:PhTimeBox width="10em" ui:field="timeBox"></ph:PhTimeBox>

    <ph:PhCalendarBox width="10em" ui:field="calBox"></ph:PhCalendarBox>
    
    <p:SmartButton text="Cal Test" ui:field="calTest"></p:SmartButton>
    
  </g:HTMLPanel>
  
</ui:UiBinder>