<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       xmlns:m="urn:import:com.googlecode.mgwt.ui.client.widget"
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:p="urn:import:it.mate.phgcommons.client.ui" 
       xmlns:ph="urn:import:it.mate.phgcommons.client.ui.ph">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="ui-homePanel">
  
    <gc:Spacer addStyleNames="ui-homeSpacer1"/>
    
    <g:HorizontalPanel width="100%" horizontalAlignment="center">
      <p:TouchAnchor addStyleNames="ui-contacts-doctor-btn" text="Doctors" ui:field="doctorsBtn">
        <ui:attribute name="text" description="text" key="doctorBtn.text"/>
      </p:TouchAnchor>
      <p:TouchAnchor addStyleNames="ui-contacts-tutor-btn" text="Tutors" ui:field="tutorsBtn">
        <ui:attribute name="text" description="text" key="tutorsBtn.text"/>
      </p:TouchAnchor>
    </g:HorizontalPanel>
    
  </g:HTMLPanel>
  
</ui:UiBinder>