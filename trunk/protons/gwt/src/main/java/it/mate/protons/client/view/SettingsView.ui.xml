<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="app-page-wrapper-panel">
  
    

    <o:OnsPage>
    
      <o:OnsToolbar>
        <div class="left">
          <o:OnsBackButton text="Back"/>
        </div>
        <div class="center">Second Toolbar</div>
        <div class="right">
          <o:OnsToolbarButton ui:field="btnRight"><o:OnsIcon icon="fa-bars"/></o:OnsToolbarButton>
        </div>
      </o:OnsToolbar>
  
      <div>Settings view!</div>  
      
      <o:OnsButton text="Pop" ui:field="btnPop"></o:OnsButton>
      
      <o:OnsButton text="Sub settings" ui:field="btnSubSettings"></o:OnsButton>
      
      <o:OnsLabel ui:field="counterLbl"/>
      
    </o:OnsPage>
    
  </g:HTMLPanel>
  
</ui:UiBinder>