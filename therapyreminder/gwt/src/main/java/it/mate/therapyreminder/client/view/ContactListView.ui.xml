<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       xmlns:m="urn:import:com.googlecode.mgwt.ui.client.widget"
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:p="urn:import:it.mate.phgcommons.client.ui" 
       xmlns:ph="urn:import:it.mate.phgcommons.client.ui.ph">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="ui-homePanel ui-TherapyListView">
    
    <g:HorizontalPanel addStyleNames="ui-top-button-bar" spacing="0">
      <p:TouchButton text="New" ui:field="newBtn" addStyleNames="ui-top-action-btn"/>
      <p:TouchButton text="Delete" ui:field="deleteBtn" addStyleNames="ui-top-action-btn"/>
    </g:HorizontalPanel>
    
    <m:ScrollPanel ui:field="resultsPanel" addStyleNames="ui-results-panel"/>
    
  </g:HTMLPanel>
  
</ui:UiBinder>